public class Room {
	// variables
	int length;
	int width;
	int height;
	String color;
	int roomNo;
	
	Room(int length, int breadth, int height){
		
	}
	
	//methods
	int calculateFloorArea() {
		int floorArea = length * width;
		return floorArea;
	}
	
	int calculatePaintingCost(int rate) {
		int wallArea = (2 * length * height) + (2 * width * height);
		int paintingCost = wallArea * rate; 
		return paintingCost;
	}
}
