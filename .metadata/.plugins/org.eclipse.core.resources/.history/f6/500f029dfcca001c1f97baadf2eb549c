
public class Room {

	// variables
	int length;
	int width;
	int height;
	String color;
	int roomNo;
		
	int calculateFloorArea() {
		int floorArea = length * width;
		return floorArea;
	}
	
	int calculatePaintingCost(int rate) {
		
		int wallArea = (2 * length * height) + (2 * width * height);
		int paintingCost = wallArea * rate; 
		return paintingCost;
	}
}
